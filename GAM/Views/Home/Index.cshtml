            beforeSend: function () {
                $('#Espera').show();
            },
            success: function (data) {

                Circles.create({
                    id: 'circles-6m',
                    radius: 45,
                    value: 10,
                    maxValue: 1000,
                    width: 7,
                    text: data,
                    colors: ['#f1f1f1', '#FF9E27'],
                    duration: 400,
                    wrpClass: 'circles-wrp',
                    textClass: 'circles-text',
                    styleWrapper: true,
                    styleText: true
                })
            },
            complete: function () {
                $('#Espera').hide();
            },
            error: function (errormessage) {

            }
        });
    }

